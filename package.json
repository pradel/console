{
  "private": true,
  "scripts": {
    "build": "npm run relay-compiler; rm -rf dist && NODE_ENV=production GRAPHQL_ENDPOINT=$BACKEND_ADDR/system webpack --config webpack.config.build.js -p && cp -r static/* dist",
    "pre-deploy": "npm run dll && npm run build && cp dll/vendor.bundle.js dist/vendor.bundle.js && cp dist/index.html dist/200.html ",
    "deploy-production": "netlify deploy -p dist -s $NETLIFY_ID -t $NETLIFY_TOKEN",
    "deploy-integration": "netlify deploy -p dist -s $INTEGRATION_NETLIFY_ID -t $INTEGRATION_NETLIFY_TOKEN",
    "download-schema": "node build/downloadSchema.js",
    "deploy": "npm run pre-deploy && npm run deploy-integration && npm run deploy-production",
    "lint": "tslint \"src/**/*.ts{,x}\"",
    "lint-fix": "tslint --fix \"src/**/*.ts{,x}\"",
    "start": "GRAPHQL_ENDPOINT=$BACKEND_ADDR/system webpack-dev-server --hot --profile --history-api-fallback --host 0.0.0.0 --port 4000 --content-base dll/",
    "test": "NODE_ENV=test npm run lint && jest",
    "test-quick": "NODE_ENV=test jest",
    "test-watch": "jest --watch",
    "test-integration": "cd integration-tests && tsc && ava --serial dist/test.js",
    "dll": "webpack --config dll.webpack.config.js",
    "analyze": "source-map-explorer dist/app.*",
    "watch-relay-compiler": "relay-compiler --watch --transform relay-compiler-typescript --schema ./build/schema.graphql --src ./src --extensions ts tsx js jsx",
    "relay-compiler": "relay-compiler --transform relay-compiler-typescript --schema ./build/schema.graphql --src ./src --extensions ts tsx js jsx",
    "prettier": "prettier --single-quote --no-semi --trailing-comma all --write '*.{ts,tsx}' 'src/**/*.{ts,tsx}'",
    "precommit": "lint-staged",
    "tsc": "tsc"
  },
  "pre-push": [
    "test-quick"
  ],
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --single-quote --no-semi --trailing-comma all --write",
      "tslint",
      "git add"
    ]
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/preprocessor.js",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__mocks__/fileMock.js",
      "^.+\\.(css|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "ts",
      "js",
      "tsx",
      "json"
    ],
    "mocksPattern": ".*src.*(?:[\\/]|^)__mocks__[\\/]",
    "testPathIgnorePatterns": [
      "/lib/"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "src/**/*.tsx",
      "!**/__tests__/**"
    ],
    "setupFiles": [
      "<rootDir>/jest-setup.js",
      "<rootDir>/src/utils/polyfils.ts"
    ]
  },
